    // 用户首页链接
    "userhomeurl": function (yhlx, dwsx) {
        if (yhlx == "1") {
            return "/views/member";
        } else {
            //单位属性
            dwsx = dwsx + '';
            switch (dwsx) {
                case '3': // 3、教育部门
                case '8': // 8、学校
                    return "/views/member01";
                case '7': // 7、安监部门
                    return "/views/member02";
                case '2': // 2、客货运企业
                    return "/views/member03";
                case '1': // 1、普通单位
                    return "/views/member04";
                case '4': // 4、交通部门
                    return '/views/member05';
                // 以下这俩货一期不做
                // 5、驾校
                // 6、4S店
            }
        }
        return '/user/type/not/defined/?';
    },
    /**
     * 用户菜单栏选中
     */
    userSideBar: function (menuId) {
        //菜单id的前缀定死
        $("li#sidebar_menu_" + menuId).addClass("active");
    },
