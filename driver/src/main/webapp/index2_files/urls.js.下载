    // ===================================
    // Default ajax data format
    ajaxData: function (param, onSuccess, onError) {
        var data = $.extend({
            method: 'POST',
            dataType: 'JSON',
            cache: false,
            success: function (data) {
                if (data.code == 200 || data.code == "200") {
                    if (onSuccess) {
                        onSuccess(data);
                    }
                } else {
                    if (data.code === 401) {
                        alert(data.message);
                        location.href = "/user/m/login";
                        return;
                    }
                    if (onError) {
                        onError(data);
                    }
                }
            },
            error: function (e) {
                // TODO:: 全局错误处理
                if (onError) {
                    if (e.responseJSON) {
                        onError(e.responseJSON);
                    }
                    else {
                        var code = e.status;
                        var msg = e.statusText;

                        if (code == 0 || code == "0") {
                            msg = "系统繁忙";
                        }

                        if (code == 503 || code == "503") {
                            msg = "服务不可用";
                        }
                        onError({
                            code: code,
                            message: msg,
                            originalData: e
                        });

                    }
                }
            }
        }, param);
        return data;
    },
    getPageinfo: function (records) {
        if (records > 0) {
            return "<div class=\"mem-block data-nums\"><p>共 <span>" + records + "</span> 条数据</p></div>";
        }
    },
    getMsg: function (data) {
        if (data && data.message) {
            return data.message;
        }
        return null;
    },
    loadCSS: function (css, type) {
        if (typeof css == 'string') {
            css = [css];
        }
        for (var i = 0; i < css.length; i++) {
            var url = css[i];
            // TODO:: user dynamic css path
            if (!(/^http:\/\//i).test(url)) {
                url = '/static/css/' + url.replace(/\/\//g, '/');
            }
            $('<link href="' + url + '" rel="stylesheet" type="text/css" media="' + (type == 'print' ? 'print' : 'screen') + '" />').appendTo($('head'));
        }
    },
    /**
     * @module urlParser
     *
     * @name 获取当前url,将参数串转化成JSON类型
     * @version 2013-2-27
     */
    getParameter: function (url) {
        //q=j，这个参数是history加的，ie需要人为区分
        if (window.location.hash && window.location.hash.indexOf("q=j") > 0)
            return null;
        var urlStr = url || window.location.search,
            param = {};
        if (urlStr) {
            var urlArr = urlStr.split("?")[1].split("&");
            for (var i = urlArr.length - 1; i >= 0; i--) {
                var tempArr = urlArr[i].split("=");
                if (tempArr[0] == 'q' && tempArr[1] == 'j') {
                    return null;
                }
                param[tempArr[0]] = decodeURIComponent(tempArr[1]);
            }
            return param;
        }
        return null;
    },
    /**
     * 返回当前运行环境的域名，带protocol
     */
    getHost: function () {
        if (!window.location.origin) {
            window.location.origin = window.location.protocol + "//"
                + window.location.hostname
                + (window.location.port ? ':' + window.location.port : '');
        }
        return window.location.origin;
    },
    "fake_bottom": "" //免得每次加逗号
});